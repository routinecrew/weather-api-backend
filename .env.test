SIGN_UP_JWT_SECRET=test-signup-jwt-secret-key
SIGN_UP_JWT_EXPIRED_SEC=1800

# 암호화 설정
AES_KEY=test-environment-aes-key-32-chars-xx
SALT_ROUNDS=10