# 애플리케이션 설정
NODE_ENV=test
PORT=3001
APP_NAME=weather-service-test
TZ=Asia/Seoul

# PostgreSQL 설정
POSTGRES_HOST=localhost
POSTGRES_PORT=5433
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres-test-password
POSTGRES_DATABASE=weather_db_test
DB_POOL_MAX=5
DB_POOL_MIN=0
DB_POOL_ACQUIRE=30000
DB_POOL_IDLE=10000
ALTER_TABLE=true

